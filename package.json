{
    "name": "redm-codelens",
    "description": "Provides CodeLens for RDR2 Native invokations in RedM lua, javascript and c# code",
    "publisher": "quintend",
    "author": {
        "name": "quinten"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "bugs": {
        "url": "https://github.com/QuintenDegraeve/redm-codelens/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/QuintenDegraeve/redm-codelens.git"
    },
    "homepage": "https://github.com/QuintenDegraeve/redm-codelens/blob/master/README.md",
    "engines": {
        "vscode": "^1.66.0"
    },
    "version": "0.0.1",
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "open-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "vscode-web:prepublish": "npm run package-web",
        "compile-web": "webpack",
        "watch-web": "webpack --watch",
        "package-web": "webpack --mode production --devtool hidden-source-map"
    },
    "dependencies": {
        "@types/object-hash": "^2.2.1",
        "await-to-js": "^3.0.0",
        "axios": "^0.26.1",
        "object-hash": "^3.0.0"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "@types/vscode": "^1.66.0",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "@vscode/test-electron": "^2.1.3",
        "@vscode/test-web": "0.0.24",
        "eslint": "^8.11.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.2",
        "process": "^0.11.10",
        "ts-loader": "^9.2.8",
        "typescript": "^4.6.3",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2"
    },
    "_": "Extension manifest properties",
    "displayName": "RedM CodeLens",
    "icon": "images/icon.png",
    "browser": "./dist/web/extension.js",
    "galleryBanner": {
        "color": "#C80000",
        "theme": "dark"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:typescript",
        "onLanguage:javascript",
        "onLanguage:lua",
        "onLanguage:c",
        "onLanguage:csharp",
        "onCommand:redm-codelens.enable",
        "onCommand:redm-codelens.disable"
    ],
    "contributes": {
        "commands": [
            {
                "title": "Enable",
                "command": "redm-codelens.enable",
                "category": "RedM Natives CodeLens"
            },
            {
                "title": "Disable",
                "command": "redm-codelens.disable",
                "category": "RedM Natives CodeLens"
            }
        ],
        "configuration": {
            "properties": {
                "redm-codelens.enable": {
                    "type": "boolean",
                    "default": true
                }
            }
        }
    }
}